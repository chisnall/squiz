{
    "name": "chisnall/squiz",
    "description": "Squiz debugger for Laravel",
    "keywords": [
        "laravel",
        "debug",
        "dev"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Chisnall",
            "email": "lee.chiz.uk@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^9 || ^10 || ^11 || ^12",
        "php": "^8.0",
        "symfony/var-dumper": "^6 || ^7"
    },
    "autoload": {
        "psr-4": {
            "Chisnall\\Squiz\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Chisnall\\Squiz\\SquizServiceProvider"
            ]
        }
    }
}
